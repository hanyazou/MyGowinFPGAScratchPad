// MAX7219 on PMOD
IO_LOC "spi_clk" G10;
IO_PORT "spi_clk" PULL_MODE=UP DRIVE=8 BANK_VCCIO=3.3;
IO_LOC "stop" D10;
IO_PORT "stop" PULL_MODE=UP DRIVE=8 BANK_VCCIO=3.3;
